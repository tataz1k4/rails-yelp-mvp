<h1>Restaurants:</h1>

<div class="container">
  <% @restaurants.each do |restaurant| %>
    <h3>
    <%= link_to restaurant.name, restaurant_path(restaurant), class: "link" %>
    </h3>
    <p>
      <%= restaurant.address%>
    </p>
  <% end %>

  <%= link_to 'add restaurant', new_restaurant_path %>

</div>
